{
	"name": "saranitou",
	"version": "0.2.6",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"postbuild": "prettier --config ./.prettierrc --write --plugin-search-dir=. '{.svelte-kit,build}/**/*.{js,css,svelte}'",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --config ./.prettierrc --write --plugin-search-dir=. 'src/**/*.{svelte,css,ts,mts}'"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/adapter-node": "^1.0.0-next.70",
		"@sveltejs/kit": "latest",
		"svelte": "latest",
		"svelte-check": "latest",
		"svelte-preprocess": "latest",
		"autoprefixer": "^10.4.2",
		"postcss": "^8.4.6",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module"
}
